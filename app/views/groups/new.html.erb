
<h1>New Group</h1>


<%= form_for :group, url: groups_path do |f| %>
  <p>
    <%= f.label :title %><br>
    <%= f.text_field :title %>
  </p>
 
  <p>
    <%= f.label :subject %><br>
    <%= f.text_area :subject %>
  </p>

  <p>
    <%= f.label :description %><br>
    <%= f.text_area :description %>
  </p>
  <p>
    <%= f.label :date %><br>
    <%= f.text_area :date %>
  </p>
  <p>
    <%= f.label :location %><br>
    <%= f.text_area :location %>
  </p>
 
  <p>
    <%= f.submit %>
  </p>
<% end %>